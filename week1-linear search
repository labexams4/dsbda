l=list()
n=int(input("Enter number of elements to be inserted into list:"))
print("Enter",n," Values")
for i in range(n):
    l.append(int(input()))
s=int(input("Enter element to be searched"))
for i in range(len(l)):
    if l[i]==s:
        print(s," is found at position", i+1)
        break
else:
    print("Element is not found")
